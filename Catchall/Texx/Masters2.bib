	keywords = {Ammonium Chloride, Anesthetics, Local, Azides, Binding, Competitive, Cell Membrane, Chloroquine, Cyanides, Cycloheximide, Dinitrophenols, Epidermal growth factor, Fibroblasts, Fluorides, Humans, Iodine Radioisotopes, Kinetics, Peptides, Temperature, Trypsin Inhibitors},
	file = {Carpenter and Cohen - 1976 - 125I-labeled human epidermal growth factor. Bindin.pdf:/Users/michael/Documents/References/storage/8FQF7X86/Carpenter and Cohen - 1976 - 125I-labeled human epidermal growth factor. Bindin.pdf:application/pdf}
