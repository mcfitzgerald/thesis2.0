	pmcid = {PMC3390854},
	file = {Liu et al. - 2012 - A single ligand is sufficient to activate EGFR dim.pdf:/Users/michael/Documents/References/storage/4DA7A4NC/Liu et al. - 2012 - A single ligand is sufficient to activate EGFR dim.pdf:application/pdf}
