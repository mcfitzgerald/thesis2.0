	author = {Yarden, Y},
	urldate = {2015-10-06},
	date = {2001-09},
	keywords = {Downregulation (physiology), Endocytosis, Epidermal growth factor, {ErbB}-2, Neoplasms, Proto-oncogene proteins, Receptor, signal transduction},
	file = {Yarden - 2001 - The EGFR family and its ligands in human cancer s.pdf:/Users/michael/Documents/References/storage/ITNGXEQT/Yarden - 2001 - The EGFR family and its ligands in human cancer s.pdf:application/pdf}
}

