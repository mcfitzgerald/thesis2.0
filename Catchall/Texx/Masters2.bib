	langid = {english},
	pmid = {14732694},
	file = {Klein et al. - 2004 - A structure-based model for ligand binding and dim.pdf:/Users/michael/Documents/References/storage/83QDCEA4/Klein et al. - 2004 - A structure-based model for ligand binding and dim.pdf:application/pdf}
