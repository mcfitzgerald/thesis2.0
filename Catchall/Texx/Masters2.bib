	journaltitle = {Best Practice \& Research Clinical Endocrinology \& Metabolism},
	shortjournal = {Best Practice \& Research Clinical Endocrinology \& Metabolism},
	author = {Jia, Min and Dahlman-Wright, Karin and Gustafsson, Jan-Åke},
	urldate = {2016-03-26},
