	shortjournal = {Trends in Pharmacological Sciences},
	author = {Christopoulos, Arthur},
	urldate = {2016-01-21},
	date = {1998-09-01},
	keywords = {data analysis, ligand–receptor interaction, logistical function, Regression Analysis},
	file = {Christopoulos - 1998 - Assessing the distribution of parameters in models.pdf:/Users/michael/Documents/References/storage/MM2BJ3JG/Christopoulos - 1998 - Assessing the distribution of parameters in models.pdf:application/pdf}
}

