@article{jia_estrogen_2015,
	title = {Estrogen receptor alpha and beta in health and disease},
	volume = {29},
	issn = {1521-690X},
	url = {http://www.sciencedirect.com/science/article/pii/S1521690X15000317},
	doi = {10.1016/j.beem.2015.04.008},
	series = {Nuclear hormone receptors for the clinician},
	abstract = {Estrogen receptors alpha ({ERα}) and beta ({ERβ}) are transcription factors that are involved in the regulation of many complex physiological processes in humans. Abnormal {ER} signaling leads to development of a variety of diseases, such as cancer, metabolic and cardiovascular disease, neurodegeneration, inflammation, and osteoporosis. This review provides an overview and update on {ERα} and {ERβ} in health and disease with focus on their role in cancer and metabolic disease and in the context of recent years' success in providing genome wide data on {ER} function. Furthermore, potential clinical applications and challenges are also discussed.},
